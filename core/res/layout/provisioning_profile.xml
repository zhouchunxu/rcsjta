        <android.support.v7.widget.AppCompatEditText
            android:id="@+id/XdmServerLogin"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dip"
            android:layout_marginRight="5dip"
